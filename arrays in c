printf("enter values\n");
for(i=0;i<=2;i++)
{
    for(j=0;j<=2;j++)
    {
     scanf("%d",&arr[i][j]);
    }
 }
 printf("output array\n");
for(i=0;i<=2;i++)
{
    for(j=0;j<=2;j++)
    {
     printf("%d",arr[i][j]);
